{
  "name": "kyse",
  "description": "Manage hotkey, dictionary",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "16": "img/logo_16.png",
    "32": "img/logo_32.png",
    "64": "img/logo_64.png",
    "128": "img/logo_128.png"
  },
  "permissions": [
    "tabs",
    "https://*/",
    "http://*/"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "option_page": "html/homePage.html",
  "browser_action": {
    "default_popup": "html/popup.html"
  },
  "content_security_policy": "script-src 'self' https://103.84.253.122:9806/; object-src 'self'"
}